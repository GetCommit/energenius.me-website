							"    var index = jsonData.map(",
							"            function(filter) {",
							"                return filter.Name;",
							"            }",
							"        ).indexOf('Japan');",
							"        ",
							"    // Get the target from the list",
							"    var target = jsonData[index];",
							"    console.log(target);",
							"",
							"    pm.expect(target).to.exist;",
							"    pm.expect(target.Name).equal('Japan')",
							"});",
